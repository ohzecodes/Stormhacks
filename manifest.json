{
   "manifest_version":2,
   "name":"HoldUp",
   "description":"Catch yourself before you send something you regret",
   "version":"1.0.0",
   "browser_action":{
      "default_title":"HoldUp",
      "default_popup":"popup.html",
      "default_options":"options.html"
   },
   "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/*"],
      "js":["popup.js"]
    }
  ],

   "permissions":[
      "activeTab"
   ]
}
